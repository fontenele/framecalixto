<?xml version='1.0' encoding='iso-8859-1' ?>
<internacionalizacao>
	<entidade>
		<nome>Tarefa</nome>
		<propriedades>
		<propriedade nome='idTarefa'>
			<nome>Tarefa</nome>
			<abreviacao>Id</abreviacao>
			<descricao>Identificador da tarefa</descricao>
		</propriedade>
		<propriedade nome='idTarefaPai'>
			<nome>Tarefa Pai</nome>
			<abreviacao>Pai</abreviacao>
			<descricao>Tarefa pai</descricao>
		</propriedade>
		<propriedade nome='idTarefaPredecessora'>
			<nome>Predecessora</nome>
			<abreviacao>Pred.</abreviacao>
			<descricao>Tarefa predecessora desta</descricao>
		</propriedade>
		<propriedade nome='csTipoTarefa'>
			<nome>Tipo Tarefa</nome>
			<abreviacao>Tipo</abreviacao>
			<descricao>Tipo de tarefa</descricao>
			<dominio>
				<opcao id='1' >Administrativa</opcao>
				<opcao id='2' >Hardware</opcao>
				<opcao id='3' >Software</opcao>
				<opcao id='4' >Netware</opcao>
			</dominio>
		</propriedade>
		<propriedade nome='idCriador'>
			<nome>Criador</nome>
			<abreviacao>Criador</abreviacao>
			<descricao>criador da tarefa</descricao>
		</propriedade>
		<propriedade nome='idResponsavel'>
			<nome>Responsavel</nome>
			<abreviacao>Resp.</abreviacao>
			<descricao>Responsavel pela tarefa</descricao>
		</propriedade>
		<propriedade nome='idResponsavelAnterior'>
			<nome>Encaminhador</nome>
			<abreviacao>Enc.</abreviacao>
			<descricao>Responsavel anterior da tarefa</descricao>
		</propriedade>
		<propriedade nome='dsTarefa'>
			<nome>Descrição</nome>
			<abreviacao>Desc.</abreviacao>
