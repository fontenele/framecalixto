			<texto id="orcamentos">Orçamento %s</texto>
			<texto id="tarefasDoUsuario">Tarefas do Usuario</texto>
			<texto id="gravarTarefa">Gravar Tarefa</texto>
			<texto id="gravarAtividade">Gravar Atividade</texto>
			<texto id="listagemAtividades">Listagem de atividades realizadas para esta tarefa</texto>
			<texto id="listagemSubTarefas">Listagem de subtarefas geradas para esta tarefa</texto>
			<texto id="listagemOrcamento">Listagem de orçamentos gerados para esta tarefa</texto>
			<texto id="trilha">Trilha de tarefas</texto>
		</textos>
