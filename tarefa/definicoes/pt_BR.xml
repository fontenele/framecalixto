		<propriedade nome='dtFimPrevisto'>
			<nome>Fim Previsto</nome>
			<abreviacao>Fim Prev.</abreviacao>
			<descricao>Fim previsto da tarefa</descricao>
		</propriedade>
		<propriedade nome='dtInicio'>
			<nome>Inicio</nome>
			<abreviacao>Inicio</abreviacao>
			<descricao>Inicio da tarefa</descricao>
		</propriedade>
		<propriedade nome='dtFim'>
			<nome>Fim</nome>
			<abreviacao>Fim</abreviacao>
			<descricao>Fim da tarefa</descricao>
		</propriedade>
		<propriedade nome='csStatus'>
			<nome>Situação</nome>
			<abreviacao>Sit.</abreviacao>
			<descricao>Situação da tarefa</descricao>
			<dominio>
				<opcao id='A' >Aberta</opcao>
				<opcao id='F' >Fechada</opcao>
			</dominio>
		</propriedade>
		<propriedade nome='nrPercentual'>
			<nome>% de conclusão</nome>
			<abreviacao>%</abreviacao>
			<descricao>Percentual de conclusão da tarefa</descricao>
		</propriedade>
		<propriedade nome='nrPrioridade'>
			<nome>Prioridade</nome>
			<abreviacao>Prior.</abreviacao>
			<descricao>Nível de prioridade da tarefa</descricao>
			<dominio>
				<opcao id='1' >Urgentíssimo</opcao>
				<opcao id='2' >Urgente</opcao>
				<opcao id='3' >Alta</opcao>
				<opcao id='4' >Normal</opcao>
				<opcao id='5' >Baixa</opcao>
				<opcao id='6' >Nenhuma</opcao>
			</dominio>
		</propriedade>
